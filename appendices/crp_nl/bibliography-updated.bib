@article{hashiguchiResistanceProportionsEight2019,
  title = {Resistance Proportions for Eight Priority Antibiotic-Bacterium Combinations in {{OECD}}, {{EU}}/{{EEA}} and {{G20}} Countries 2000 to 2030: A Modelling Study},
  shorttitle = {Resistance Proportions for Eight Priority Antibiotic-Bacterium Combinations in {{OECD}}, {{EU}}/{{EEA}} and {{G20}} Countries 2000 to 2030},
  author = {Hashiguchi, Tiago Cravo Oliveira and Ouakrim, Driss Ait and Padget, Michael and Cassini, Alessandro and Cecchini, Michele},
  date = {2019-05-16},
  journaltitle = {Eurosurveillance},
