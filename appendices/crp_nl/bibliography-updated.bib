@article{ievenAetiologyLowerRespiratory2018,
  title = {Aetiology of Lower Respiratory Tract Infection in Adults in Primary Care: A Prospective Study in 11 {{European}} Countries},
  shorttitle = {Aetiology of Lower Respiratory Tract Infection in Adults in Primary Care},
  author = {Ieven, M. and Coenen, S. and Loens, K. and Lammens, C. and Coenjaerts, F. and Vanderstraeten, A. and Henriques-Normark, B. and Crook, D. and Huygen, K. and Butler, C. C. and Verheij, T. J. M. and Little, P. and Zlateva, K. and van Loon, A. and Claas, E. C. J. and Goossens, H.},
